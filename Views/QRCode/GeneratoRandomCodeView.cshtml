@model AtomHealth.Areas.Identity.Data.AtomHealthUser

@{
    ViewData["Title"] = "GeneratoRandomCodeView";
}

<h1>One Time Passcode</h1>


<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="GenerateRandomCodeViewWithCheck" asp-controller="QRCode">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
         
            <div class="form-group">
               Code Box
                <input type="text" class="form-control" name="DoctorEnteredCode" />
               
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
