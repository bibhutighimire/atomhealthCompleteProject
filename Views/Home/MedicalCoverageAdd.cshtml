@model AtomHealth.Models.MedicalCoverage

@{
    ViewData["Title"] = "MedicalCoverageAdd";
}


<h4>Add Medical Coverage</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="MedicalCoverageAddNew">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            <div class="form-group">
                <label asp-for="HealthCarePlan" class="control-label"></label>
                <input asp-for="HealthCarePlan" class="form-control" />
                <span asp-validation-for="HealthCarePlan" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Coverage" class="control-label"></label>
                <input asp-for="Coverage" class="form-control" />
                <span asp-validation-for="Coverage" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HealthID" class="control-label"></label>
                <input asp-for="HealthID" class="form-control" />
                <span asp-validation-for="HealthID" class="text-danger"></span>
            </div>
          
           
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
